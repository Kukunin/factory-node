{
  "name": "factory-node",
  "version": "0.0.1",
  "description": "FactoryBot-like factories to JavaScript/TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kukunin/factory-node.git"
  },
  "keywords": [
    "factories",
    "jest",
    "testing",
    "tdd",
    "javascript",
    "prisma"
  ],
  "author": "Sergiy Kukunin <sergey.kukunin@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kukunin/factory-node/issues"
  },
  "homepage": "https://github.com/Kukunin/factory-node#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@types/jest": "29.5.13",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "eslint": "9.12.0",
    "eslint-plugin-jest": "28.8.3",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "typescript": "5.6.2"
  },
  "files": [
    "lib/**/*"
  ]
}
